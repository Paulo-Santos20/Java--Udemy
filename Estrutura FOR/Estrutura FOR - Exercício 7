import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int n;

		n = sc.nextInt();

		for (int x = 1; x <= n; x++) {
			int linha = x;
			int quadrado = x * x;
			int cubo = x * x * x;
			System.out.printf("%d %d %d%n", linha, quadrado, cubo);
		}

		sc.close();
	}
}
